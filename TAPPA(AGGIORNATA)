CREATE TABLE TAPPA (
    
    ID_T SMALLINT AUTO_INCREMENT PRIMARY KEY,
	ID_TRAGITTO SMALLINT,
    CITTA VARCHAR(20) NOT NULL,
    VIA VARCHAR(30) NOT NULL,
    LAT FLOAT(10, 6) ,
    LNG FLOAT(10, 6) ,
    ORARIO_ARRIVO DATETIME NOT NULL,
    
    FOREIGN KEY (ID_TRAGITTO) REFERENCES TRAGITTO(ID)
    
    ) ENGINE=INNODB;
